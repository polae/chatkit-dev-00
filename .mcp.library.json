{
  "mcpServers": {
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "figma-desktop": {
      "url": "http://127.0.0.1:3845/mcp",
      "type": "http"
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "NanoBanana": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/johnsteven/GITHUB/MCP/mcp-servers/mcps/nano-mcp",
        "run",
        "server.py"
      ]
    },
    "GPTChing": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/johnsteven/GITHUB/MCP/mcp-servers/mcps/gptching-mcp",
        "run",
        "server.py"
      ]
    }
  }
}
